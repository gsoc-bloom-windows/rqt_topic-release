<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rqt-topic</id>
    <version>1.0.0</version>
    <title>ros-dashing-rqt-topic</title>
    <authors>Dirk Thomas <dthomas@osrfoundation.org></authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>rqt_topic provides a GUI plugin for displaying debug information about ROS topics including publishers, subscribers, publishing rate, and ROS Messages.</description>
<dependencies>
      <dependency id="ros-dashing-python-qt-binding"/>
      <dependency id="ros-dashing-rqt-gui"/>
      <dependency id="ros-dashing-rqt-gui-py"/>
      <dependency id="ros-dashing-rqt-py-common"/>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>